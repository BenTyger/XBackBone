{
  "name": "sergix44/xbackbone",
  "version": "2.0",
  "description": "A lightweight ShareX PHP backend",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "slim/slim": "^3.0",
    "slim/twig-view": "^2.4",
    "league/flysystem": "^1.0.45",
    "monolog/monolog": "^1.23",
    "ext-json": "*",
    "ext-gd": "*",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "extra": {
    "violinist": {
      "allow_updates_beyond_constraint": 1,
      "blacklist": [],
      "update_with_dependencies": 1
    }
  }
}
